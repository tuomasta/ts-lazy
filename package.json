{
  "name": "ts-lazy",
  "version": "0.0.1",
  "description": ".Net lazy for typescript",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "pretest": "npm run build",
    "test": "npm run jasmine",
    "prebuild": "npm run clean && npm run lint",
    "build": "npm run compile",
    "lint": "tslint lib/**/*.ts",
    "compile": "tsc",
    "clean": "rimraf lib/**/*.js && rimraf lib/**/*.d.ts && rimraf lib/**/*.js.map",
    "jasmine": "jasmine JASMINE_CONFIG_PATH=./jasmine.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tuomasta/ts-lazy.git"
  },
  "keywords": [
    ".net",
    "typescript",
    "lazy"
  ],
  "author": "Tuomas Talus",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/tuomasta/ts-lazy/issues"
  },
  "homepage": "https://github.com/tuomasta/ts-lazy#readme",
  "devDependencies": {
    "@types/jasmine": "^2.5.40",
    "rimraf": "^2.5.4",
    "tslint": "^4.3.1",
    "typescript": "^2.1.4"
  }
}